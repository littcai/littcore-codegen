            <param value="2" content="新闻" remark=""/>
            <param value="3" content="通知" remark=""/>
        </type>
    </module>

    <module table="CUSTOMER">
        <type code="customer-type" name="Customer Type" column="TYPE">
            <param value="1" content="销售客户" default="true" />
            <param value="2" content="供应商" />
            <param value="3" content="制造商" />
